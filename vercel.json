{
  "version": 2,
  "routes": [
    { "src": "/admin", "dest": "/index.html" },
    { "src": "/check-in", "dest": "/index.html" },
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/static/(.*)", "headers": { "cache-control": "s-maxage=31536000,immutable" } },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
